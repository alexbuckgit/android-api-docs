    <Member MemberName="SetListShown">
      <MemberSignature Language="C#" Value="public virtual void SetListShown (bool shown);" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance void SetListShown(bool shown) cil managed" />
      <MemberType>Method</MemberType>
