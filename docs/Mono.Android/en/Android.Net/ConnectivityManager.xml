    <Member MemberName="ConnectivityAction">
      <MemberSignature Language="C#" Value="public const string ConnectivityAction;" />
      <MemberSignature Language="ILAsm" Value=".field public static literal string ConnectivityAction" />
      <MemberType>Field</MemberType>
      <AssemblyInfo>
