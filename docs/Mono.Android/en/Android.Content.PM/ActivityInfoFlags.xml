    <Member MemberName="ClearTaskOnLaunch">
      <MemberSignature Language="C#" Value="ClearTaskOnLaunch" />
      <MemberSignature Language="ILAsm" Value=".field public static literal valuetype Android.Content.PM.ActivityInfoFlags ClearTaskOnLaunch = int32(4)" />
      <MemberType>Field</MemberType>
      <AssemblyInfo>
