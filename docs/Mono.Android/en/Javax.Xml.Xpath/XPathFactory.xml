    <Member MemberName="NewInstance">
      <MemberSignature Language="C#" Value="public static Javax.Xml.Xpath.XPathFactory NewInstance (string uri);" />
      <MemberSignature Language="ILAsm" Value=".method public static hidebysig class Javax.Xml.Xpath.XPathFactory NewInstance(string uri) cil managed" />
      <MemberType>Method</MemberType>
      <AssemblyInfo>
        <AssemblyVersion>0.0.0.0</AssemblyVersion>
