      <MemberSignature Language="C#" Value="public virtual string[][] GetZoneStrings ();" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance string[][] GetZoneStrings() cil managed" />
      <MemberType>Method</MemberType>
      <AssemblyInfo>
