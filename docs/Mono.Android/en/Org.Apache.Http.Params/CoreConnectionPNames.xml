    <Member MemberName="TcpNodelay">
      <MemberSignature Language="C#" Value="public const string TcpNodelay;" />
      <MemberSignature Language="ILAsm" Value=".field public static literal string TcpNodelay" />
      <MemberType>Field</MemberType>
      <AssemblyInfo>
