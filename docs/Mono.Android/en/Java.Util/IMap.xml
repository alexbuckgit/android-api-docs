    <Member MemberName="Values">
      <MemberSignature Language="C#" Value="public System.Collections.ICollection Values ();" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance class System.Collections.ICollection Values() cil managed" />
      <MemberType>Method</MemberType>
