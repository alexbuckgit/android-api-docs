      <Attributes>
        <Attribute>
          <AttributeName>Android.Runtime.Register("values", "()Ljava/util/Collection;", "GetValuesHandler:Java.Util.IMapInvoker, Mono.Android, Version=0.0.0.0, Culture=neutral, PublicKeyToken=null")</AttributeName>
