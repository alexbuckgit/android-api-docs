    <Member MemberName="ActionQueryContentRatingSystems">
      <MemberSignature Language="C#" Value="public const string ActionQueryContentRatingSystems;" />
      <MemberSignature Language="ILAsm" Value=".field public static literal string ActionQueryContentRatingSystems" />
      <MemberType>Field</MemberType>
      <AssemblyInfo>
