    <Member MemberName="ParentId">
      <MemberSignature Language="C#" Value="public string ParentId { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance string ParentId" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
