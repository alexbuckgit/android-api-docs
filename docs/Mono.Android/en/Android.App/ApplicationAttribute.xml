  <TypeSignature Language="C#" Value="public sealed class ApplicationAttribute : Attribute" />
  <TypeSignature Language="ILAsm" Value=".class public auto ansi serializable sealed beforefieldinit ApplicationAttribute extends System.Attribute" />
  <AssemblyInfo>
