        <format type="text/html">
          <a href="http://developer.android.com/reference/android/view/textservice/SentenceSuggestionsInfo.html" target="_blank">[Android Documentation]</a>
        </format>
      </para>
    </remarks>
    <since version="Added in API level 16" />
  </Docs>
